{
  "name": "react18-ssg",
  "version": "1.0.0",
  "description": "Static Site Generator - React 18 App",
  "scripts": {
    "bundle:client": "webpack --config webpack.client.config.js",
    "bundle:server": "webpack --config webpack.server.config.js",
    "bundle": "yarn bundle:client && yarn bundle:server",
    "render": "node src/prerender.js",
    "build": "yarn bundle && yarn render",
    "start": "serve -s dist"
  },
  "keywords": [
    "React 18",
    "SSG"
  ],
  "author": "Mateusz Matysiak",
  "license": "ISC",
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "mini-css-extract-plugin": "^2.6.0",
    "serve": "^13.0.2",
    "webpack-node-externals": "^3.0.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
